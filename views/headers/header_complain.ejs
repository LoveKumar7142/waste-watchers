      <nav id="navbar" class="navbar">
        <ul>
          <li><a class="nav-link scrollto active" href="/"><span class="fa fa-home"> Home </span></a></li>
          <li><a class="nav-link scrollto" href="/"><span class="fa fa-info-circle" aria-hidden="true"> About us</span></a></li>
      
          <!-- Conditional Navigation based on Login and Role -->
          <% if (isLoggedIn) { %>
